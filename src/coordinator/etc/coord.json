{
  "port": 8791,
  "cluster": {
    "nodes": "${TASKMANAGER_NODES}",
    "rpc_timeout": 3,
    "connect_timeout": 3
  },
  "storage": {
    "Local": {
      "dataflow_store_path": "${HOME}/lightflus/dataflow"
    }
  },
  "heartbeat": {
    "nodes": "${TASKMANAGER_NODES}",
    "period": 3,
    "connect_timeout": 3,
    "rpc_timeout": 3
  },
  "ack": {
    "delay": 1,
    "buf_size": 500,
    "nodes": "${TASKMANAGER_NODES}",
    "connect_timeout": 3,
    "rpc_timeout": 3
  }
}